      "scripts": {
        "pre_animation": [
          "variable.helmet_layer_visible = 1.0;",
          "variable.leg_layer_visible = 1.0;",
          "variable.boot_layer_visible = 1.0;",
          "variable.chest_layer_visible = 1.0;",
          "variable.attack_body_rot_y = Math.sin(360*Math.sqrt(variable.attack_time)) * 5.0;",
          "variable.tcos0 = (math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
          "variable.first_person_rotation_factor = math.sin((1 - variable.attack_time) * 180.0);",
          "variable.hand_bob = query.life_time < 0.01 ? 0.0 : variable.hand_bob + ((query.is_on_ground && query.is_alive ? math.clamp(math.sqrt(math.pow(query.position_delta(0), 2.0) + math.pow(query.position_delta(2), 2.0)), 0.0, 0.1) : 0.0) - variable.hand_bob) * 0.02;",
          "variable.map_angle = math.clamp(1 - variable.player_x_rotation / 45.1, 0.0, 1.0);",
          "variable.item_use_normalized = query.main_hand_item_use_duration / query.main_hand_item_max_duration;" , //add comma for addon
	  
	  //addon pre_animation
	  "variable.chunk_x = (8-math.mod(query.position(0),16) + (query.position(0) <= 0 ? -16))*16;",
	  "variable.chunk_y = (1.6-math.mod(query.position(1),16) + (query.position(1) <= 0 ? -16))*16;",
	  "variable.chunk_z = (-8+math.mod(query.position(2),16) + (query.position(2) <= 0 ? 16))*16;",
	  
	  "variable.info_screen_var_show_item = query.get_equipped_item_name(1) == 'arrow';",
	  "variable.info_screen_var_full_show = variable.is_first_person && !query.is_gliding && !query.is_swimming && !query.is_riding && variable.info_screen_var_show_item;",
	  "variable.info_screen_var_full_show_always = variable.is_first_person && !query.is_gliding && !query.is_swimming && !query.is_riding;",
	  "variable.info_screen_var_ui_show = query.is_in_ui && !query.is_gliding && !variable.info_screen_var_show_item;",
	  
	  "variable.info_screen_var_biome = query.variant;",
	  
	  "variable.info_screen_var_cardinal = query.cardinal_facing_2d;",
	  
	  "variable.info_screen_var_clock_ampm = query.time_of_day*2;",
	  "variable.info_screen_var_clock_h_0 = query.time_of_day *24;",
	  "variable.info_screen_var_clock_h_10 = query.time_of_day *24;",
	  "variable.info_screen_var_clock_m_0 = 0;", //count by ten minutes
	  //"variable.info_screen_var_clock_m_0 = query.time_of_day *1440;", //count by minutes
	  "variable.info_screen_var_clock_m_10 = query.time_of_day *144;",
	  
	  "variable.info_screen_var_distance_10 = (query.walk_distance*1.65)/10000;",
	  "variable.info_screen_var_distance_0 = (query.walk_distance*1.65)/1000;",
	  "variable.info_screen_var_distance_00 = (query.walk_distance*1.65)/100;",
	  
	  "variable.info_screen_var_speed_10 = query.ground_speed/10;",
	  "variable.info_screen_var_speed_0 = query.ground_speed;",
	  "variable.info_screen_var_speed_00 = query.ground_speed*10;",
	  
	  "variable.info_screen_var_fps_10 = math.round(0.1/query.delta_time);",
	  "variable.info_screen_var_fps_0 = math.round(((0.1/query.delta_time)-math.round(0.1/query.delta_time))*10);",
	  
	  "variable.info_screen_var_tps_10 = query.skin_id/10;",
	  "variable.info_screen_var_tps_0 = query.skin_id;",
	  
	  "variable.info_screen_var_light_10 = query.mark_variant/10;",
	  "variable.info_screen_var_light_0 = query.mark_variant;",
	  
	  "variable.info_screen_var_day_1000 = query.day/1000;",
	  "variable.info_screen_var_day_100 = query.day/100;",
	  "variable.info_screen_var_day_10 = query.day/10;",
	  "variable.info_screen_var_day_0 = query.day;",
	  
	  "variable.info_screen_var_hours_100 = math.floor(query.day/300);",
	  "variable.info_screen_var_hours_10 = math.floor(query.day/30);",
	  "variable.info_screen_var_hours_0 = math.floor(query.day/3);",
	  "variable.info_screen_var_hours_00 = math.floor(query.day*2);",
	  
	  "variable.info_screen_var_chunknum_x_100 = math.abs(math.floor(query.position(0)/16)/100);",
	  "variable.info_screen_var_chunknum_x_10 = math.abs(math.floor(query.position(0)/16)/10);",
	  "variable.info_screen_var_chunknum_x_0 = math.abs(math.floor(query.position(0)/16));",
	  "variable.info_screen_var_chunknum_x_neg = (math.floor(query.position(0)/16)) < 0;",
	  "variable.info_screen_var_chunknum_z_100 = math.abs(math.floor(query.position(2)/16)/100);",
	  "variable.info_screen_var_chunknum_z_10 = math.abs(math.floor(query.position(2)/16)/10);",
	  "variable.info_screen_var_chunknum_z_0 = math.abs(math.floor(query.position(2)/16));",
